<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--昼夜模式主色调-->
<attr name="dayNightBackground" format="color|reference" />
<attr name="dayNightOneTextColor" format="color|reference" />
<attr name="dayNightTwoTextColor" format="color|reference" />
<attr name="dayNightThreeTextColor" format="color|reference" />
<attr name="dayNightBackgroundDrawable" format="color|reference" />

<declare-styleable name="DayNightMode">
    <attr name="dayNightBackgroundAttr" format="string|reference"/><!--切换背景属性-->
    <attr name="dayNightOneTextColorAttr" format="string|reference"/><!--切换一级字体颜色属性-->
    <attr name="dayNightTwoTextColorAttr" format="string|reference"/><!--切换二级字体颜色属性-->
    <attr name="dayNightThreeTextColorAttr" format="string|reference"/><!--切换三级字体颜色属性-->
    <attr name="dayNightBackgroundDrawableAttr" format="string|reference"/><!--切换背景图片属性-->

</declare-styleable>
    <declare-styleable name="SpaceTabLayout">
        <attr name="number_of_tabs">
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
            <enum name="three_IconOnly" value="0" />
        </attr>

        <attr name="starting_position">
            <enum name="one" value="0"/>
            <enum name="two" value="1"/>
            <enum name="three" value="2"/>
            <enum name="four" value="3"/>
            <enum name="five" value="4"/>
        </attr>

        <attr name="tab_color" format="color" />
        <attr name="button_color" format="color" />
        <attr name="text_color" format="color" />

        <attr name="icon_one" format="reference" />
        <attr name="icon_two" format="reference" />
        <attr name="icon_three" format="reference" />
        <attr name="icon_four" format="reference" />
        <attr name="icon_five" format="reference" />

        <attr name="text_one" format="string" />
        <attr name="text_two" format="string" />
        <attr name="text_three" format="string" />
    </declare-styleable>

</resources>